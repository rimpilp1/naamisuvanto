			globals.is_business = false;
			globals.use_storyboard_editor = false;

			// handle browser back/forward
			//  the load event -> timeout -> popstate event method is a hack because older chromes / safari (webkit) fires popstate on document load
			window.addEventListener('load', function() {
				setTimeout(function() {
					window.addEventListener('popstate', function() {
						var scope = angular.element($("#magisto_video_feed_cont")).scope();
						scope.$apply(function(){
							scope.on_popstate();
						});
					});
				}, 0);
			});

			// show vimeo claim popup - with param in url vimeo_connect=1
            if(common.get_url_param('vimeo_connect')==1){
              if(!globals.connected_to_vimeo && (globals.vimeo_account_eligibility=='business' || globals.vimeo_account_eligibility=='pro')){
                setTimeout(function(){
                  common.deferred_popup('vimeo-claim-popup', 'vimeo_claim_popup',{});
