return credit;}
$scope.gettyUpsell.checkUserCredits=function(){return plansService.get_user_stock_credits().then(function(res){if(res){$scope.gettyUpsell.purchased_credit=parseInt(res.purchased_credit);$scope.gettyUpsell.used_stock_credit=parseInt(res.used_stock_credit);return $scope.set_user_credits(parseInt(res.stock_credit),true);}});};$scope.gettyUpsell.clear_interval=function(){if($scope.gettyUpsell.interval_id)
